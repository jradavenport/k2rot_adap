	Author = {{Sahu}, K.~C. and {Bond}, H.~E. and {Anderson}, J. and {Dominik}, M.},
	Date-Added = {2016-10-29 05:03:06 +0000},
	Date-Modified = {2016-10-29 05:03:08 +0000},
	Doi = {10.1088/0004-637X/782/2/89},
	Eid = {89},
	Eprint = {1401.0239},
	Journal = {\apj},
	Keywords = {astrometry, gravitational lensing: micro, planetary systems, stars: individual: Proxima Centauri},
