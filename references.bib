@inproceedings{vogt1981,
	Abstract = {This paper presents a brief review of the current understanding of spots on BY Dra and RS CVn-type variables. Methods for determining various physical parameters of the spots, such as sizes, temperatures, magnetic fields, and migratory motions are reviewed in detail. Two new observational programs currently underway at Lick Observatory to study spot-related activity are also described. The first involves direct measurements of magnetic fields on chromospherically-active late-type dwarfs. The second program involves actual imaging of starspots on the stellar disk through high resolution, high signal-to-noise studies of rotationally broadened spectral lines.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
