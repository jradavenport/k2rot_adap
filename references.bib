	Doi = {10.1086/176266},
	Journal = {\apj},
	Keywords = {STARS: ACTIVITY, STARS: FLARE, STARS: INDIVIDUAL CONSTELLATION NAME: CN LEONIS, ULTRAVIOLET: STARS},
	Month = oct,
	Pages = {795},
