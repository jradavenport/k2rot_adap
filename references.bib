	Date-Modified = {2015-01-14 00:46:49 +0000},
	Doi = {10.1051/0004-6361:20042475},
	Eprint = {astro-ph/0510075},
	Journal = {\aap},
	Keywords = {magnetohydrodynamics (MHD), turbulence, stars: activity, stars: chromosphere, stars: low mass, brown dwarfs},
	Month = feb,
	Pages = {1027-1037},
	Title = {{Large-scale {$\alpha$}\^{}2-dynamo in low-mass stars and brown dwarfs}},
	Volume = 446,
	Year = 2006,
