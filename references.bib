	Eprint = {1405.1453},
	Journal = {ArXiv e-prints 1405.1453},
	Keywords = {Astrophysics - Solar and Stellar Astrophysics, Physics - Plasma Physics, Physics - Space Physics},
	Month = may,
	Primaryclass = {astro-ph.SR},
