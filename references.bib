	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1405.0503},
	Author = {{Schultheis}, M. and {Chen}, B.~Q. and {Jiang}, B.~W. and {Gonzalez}, O.~A. and {Enokiya}, R. and {Fukui}, Y. and {Torii}, K. and {Rejkuba}, M. and {Minniti}, D.},
	Date-Added = {2014-06-18 01:37:27 +0000},
	Date-Modified = {2014-06-18 01:37:27 +0000},
	Eprint = {1405.0503},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Astrophysics of Galaxies, Astrophysics - Solar and Stellar Astrophysics},
