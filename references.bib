	Doi = {10.1086/165647},
	Journal = {\apj},
	Keywords = {Doppler Effect, Image Reconstruction, Maximum Entropy Method, Peculiar Stars, Stellar Rotation, Stellar Spectra, Computational Grids, Iterative Solution, Line Spectra, Signal To Noise Ratios, Spatial Resolution, Spectral Resolution, Starspots},
