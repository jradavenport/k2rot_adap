	Eprint = {astro-ph/0208546},
	Journal = {\apj},
	Keywords = {Methods: Data Analysis, Methods: Statistical, Stars: Coronae, Stars: Flare, Stars: Late-Type, X-Rays: Stars},
	Month = dec,
