@article{bertelli1999,
	Abstract = {In this paper we examine the star formation history inferred from the Hipparcos color-magnitude diagram (CMD) of field stars in the solar vicinity and check whether the whole galactic disk has formed stars in a similar fashion. We find that the properties of the CMD in question are very local and the underlying star formation history cannot hold for the whole disk. With the expected capability of Gaia we examine how far in space Gaia would be able to sample stars with the same precision of parallaxes as Hipparcos. We find that a minimum coverage of about 2 kpc from the Sun is easily achievable. In such a case the star formation history, derived from the Gaia data, is expected to represent the past star forming activity over the whole galactic disk.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/1999BaltA...8..271B},
	Author = {{Bertelli}, G. and {Bressan}, A. and {Chiosi}, C. and {Vallenari}, A.},
	Date-Added = {2016-09-18 01:07:19 +0000},
	Date-Modified = {2016-09-18 01:07:19 +0000},
	Journal = {Baltic Astronomy},
	Keywords = {GALAXY: DISK, STARS: FORMATION HISTORY, HR DIAGRAM, ORBITING OBSERVATORIES: HIPPARCOS, GAIA},
	Pages = {271-283},
