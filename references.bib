	Doi = {10.1134/S1063773712010033},
	Journal = {Astronomy Letters},
	Keywords = {Galactic solar neighborhood, characteristics and properties of the Milky Way Galaxy, interstellar medium, nebulae in the Milky Way},
	Month = feb,
	Pages = {87-100},
	Title = {{3D interstellar extinction map within the nearest kiloparsec}},
	Volume = 38,
	Year = 2012,
	Bdsk-Url-1 = {http://dx.doi.org/10.1134/S1063773712010033},
